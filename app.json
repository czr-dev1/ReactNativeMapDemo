{
  "expo": {
    "privacy": "unlisted",
    "name": "MapDemo",
    "slug": "MapDemo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/color_icon.png",
    "splash": {
      "image": "./app/assets/color_splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.thearqive.thearqivemobile",
      "buildNumber": "0.1.0"
    },
    "android": {
      "softwareKeyboardLayoutMode": "pan",
      "package": "com.thearqive.thearqivemobile",
      "versionCode": 1,
      "permissions": ["INTERNET"]
    },
    "web": {
      "favicon": "./app/assets/favicon.png"
    }
  }
}
